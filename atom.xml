  <entry>
    <title>http协议</title>
    <link href="https://yngty.github.io/2023/07/17/network/http%E5%8D%8F%E8%AE%AE/"/>
    <id>https://yngty.github.io/2023/07/17/network/http%E5%8D%8F%E8%AE%AE/</id>
    <published>2023-07-17T11:42:24.000Z</published>
